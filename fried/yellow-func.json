{
    "files.autoSave": "afterDelay",
    "workbench.colorTheme": "Tokyo Night",
    "editor.guides.bracketPairs": true,
    "terminal.integrated.fontFamily": "MesloLGM Nerd Font Mono",
    "editor.cursorStyle": "line-thin",
    "telemetry.telemetryLevel": "off",
    "window.zoomLevel": 1,
    "javascript.suggest.autoImports": false,
    "typescript.suggest.autoImports": false,
    "editor.mouseWheelScrollSensitivity": 2,
    "editor.parameterHints.enabled": false,
    "editor.hover.delay": 200,

    "[html]": {
        "editor.tabSize": 2
    },
    "[css]": {
        "editor.tabSize": 2
    },
    "[javascript]": {
        "editor.tabSize": 2,
        "editor.rulers": [
            80
            ]
    },
    "[javascriptreact]": {
        "editor.cursorBlinking": "smooth",
        "editor.tabSize": 2,
        "editor.rulers": [
            80
            ]
    },
    "workbench.colorCustomizations": {
        "editor.background": "#161316",
        "gitDecoration.untrackedResourceForeground": "#aaaacc",
        "gitDecoration.modifiedResourceForeground": "#7c6ce8",
        "statusBar.background": "#070724",
        "statusBar.foreground": "#114fb9",
        "activityBar.background": "#121422",
        "activityBar.foreground": "#7e7e95",
        "activityBarBadge.foreground": "#93b0a2",
        "activityBarBadge.background": "#004488",
        "editor.lineHighlightBackground": "#020222",
        "editor.selectionBackground": "#3a3a9d",
        "editor.selectionHighlightBackground": "#dc580032",
        "editor.wordHighlightBackground": "#4a39ff58",
        "list.hoverBackground": "#0062766d",
        "list.activeSelectionBackground": "#006276",
        "sideBar.background": "#121418",
        "sideBar.foreground": "#5f5f7d",
        "sideBarSectionHeader.background": "#121422",
        "sideBarTitle.foreground": "#8888cc",
        "editorBracketHighlight.foreground1": "#3e3eda",
        "editorBracketHighlight.foreground2": "#983de3",
        "editorBracketHighlight.foreground3": "#c845e9",
        "editorBracketHighlight.foreground4": "#e35688",
        "editorBracketHighlight.foreground5": "#ed9741",
        "editorBracketHighlight.foreground6": "#f3b63c"
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
          {
            "scope": [
                "keyword.control",
                "keyword.operator.expression",
                "keyword.operator.new",
                "storage"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#9D7CD8"
            }
          },
          {
            "scope" : [
                "string.quoted.single.js",
                "string.quoted.double.js",
                "punctuation.definition.string.begin.js",
                "punctuation.definition.string.end.js",
                "punctuation.definition.string.template.begin.js",
                "punctuation.definition.string.template.end.js",
                "string.template.js"
             ],
             "settings":{
                "foreground": "#908de0"
             }
          },  
          {
            "scope" : [
                "keyword.control",
                "keyword.operator",
                "keyword.operator.bitwise.shift.js",
                "keyword.operator.relational.js",
                "keyword.operator.comparison.js",
                "keyword.operator.logical.js",
                "keyword.operator.expression",
                "keyword.operator.new",
                "storage.type.function.arrow.js",
                "storage"
             ],
             "settings":{
                "foreground": "#8a72d7"
             }
          },
          {
            "scope" : [
                "variable.other.readwrite.js",
                "variable.other.object.js",
                "variable.other.object.property.js",
                "variable.other.property.js",
                "punctuation"
             ],
             "settings" : {
                "foreground": "#b3b8f5"
             }
          },
          {
            "scope" : [
                "none.but.its.pretty"
            ],
            "settings" : {
                "foreground": "#6382ff"
            }
          },
          {
            "scope" : [
                "variable.other.constant"
            ],
            "settings" : {
                "foreground": "#e17663"
            }
          },
          {
            "scope" : [
                "meta.object-literal.key"
            ],
            "settings" : {
                "foreground": "#8e70f8"
            }
          },
          {
            "scope" : [
                "variable.language.this.js"
            ],
            "settings" : {
                "foreground": "#e46e95"
            }
          },
          
          {
            "scope" : [
                "entity.name.function.js"
            ],
            "settings" : {
                "foreground": "#f8b82b"
            }
          }
        ]
    },
    "editor.minimap.renderCharacters": false,
    "editor.minimap.enabled": false,
    "editor.fontFamily": "'Roboto Mono', 'Hack', 'Wingdings', Consolas, 'Courier New', monospace",
    "editor.fontWeight": 400,
    "editor.fontSize": 14
}
